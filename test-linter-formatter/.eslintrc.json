{
    "env": {
        "browser": true,
        "es2021": true
    },

    "extends": "airbnb-base",

    "overrides": [
    ],

    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "rules": {
        "eol-last": "off",
        "no-console": "off",
        "linebreak-style": "off",
        "no-unused-vars": "off",
        "quotes": ["warn", "double"],
        "indent": ["error", 4]
    }
}
